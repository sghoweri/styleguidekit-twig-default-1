<!-- View All Patterns in a Pattern Sub-Type -->
<div class="o-wrapper u-1/1">
  {% grid %}
    {% cell "u-1/1 u-1/3@small u-1/4@medium" %}
      <div class="sg-main sg-sticky-nav">
        <ul class="o-ui-list">
          {% for partial in partials if partial.patternSectionSubtype == false %}
          <li class="o-ui-list__item">
            <a href="#{{ partial.patternPartial }}">{{ partial.patternName }}</a>
          </li>
          {% endfor %}
        </ul>
      </div>
    {% endcell %}

    {% cell "u-1/1 u-2/3@small u-3/4@medium" %}
      <div class="sg-main" role="main">
        <!-- Patterns -->
        <div id="sg-patterns">
          {% for partial in partials %}
            {% if partial.patternSectionSubtype %}
              {% include "patternSectionSubtype.twig" %}
            {% else %}
              {% include "patternSection.twig" %}
            {% endif %}
          {% endfor %}
        </div> <!--end #sg-patterns-->
      </div><!--end .sg-main-->
    {% endcell %}
  {% endgrid %}
</div>